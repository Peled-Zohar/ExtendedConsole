<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Zohar Peled</Authors>
    <Company />
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Provides a set of methods to make your life as a programmer easier when writing a Console application.</Description>
    <PackageLicenseUrl>https://github.com/Peled-Zohar/ExtendedConsole/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Peled-Zohar/ExtendedConsole</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Peled-Zohar/ExtendedConsole</RepositoryUrl>
    <Copyright />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>7.3</LangVersion>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Version>1.0.2</Version>
    <PackageIconUrl>https://github.com/Peled-Zohar/ExtendedConsole/blob/master/extendedconsole.ico</PackageIconUrl>
    <ApplicationIcon>extendedconsole.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Development\ExtendedConsole\ExtendedConsole\ExtendedConsole.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>C:\Development\ExtendedConsole\ExtendedConsole\ExtendedConsole.xml</DocumentationFile>
  </PropertyGroup>

</Project>
